{
  "package": {
    "productName": "linksaas",
    "version": "0.2.31"
  },
  "build": {
    "distDir": "../dist",
    "devPath": "http://localhost:3000",
    "beforeDevCommand": "yarn run start",
    "beforeBuildCommand": "yarn run build",
    "withGlobalTauri": true
  },
  "tauri": {
    "bundle": {
      "active": true,
      "targets": [
        "appimage",
        "msi",
        "app",
        "updater"
      ],
      "identifier": "pro.linksaas.desktop",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "resources": [],
      "externalBin": [],
      "copyright": "",
      "category": "DeveloperTool",
      "shortDescription": "",
      "longDescription": "",
      "deb": {
        "depends": []
      },
      "macOS": {
        "frameworks": [],
        "minimumSystemVersion": "",
        "exceptionDomain": "",
        "signingIdentity": null,
        "providerShortName": null,
        "entitlements": null
      },
      "windows": {
        "tsp": false,
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": "",
        "wix": {
          "language": "zh-CN",
          "bannerPath": "assets/wix-banner.bmp",
          "dialogImagePath": "assets/wix-dialog.bmp"
        },
        "webviewInstallMode": {
          "type": "embedBootstrapper"
        }
      }
    },
    "updater": {
      "active": false
    },
    "allowlist": {
      "window": {
        "all": true
      },
      "shell": {
        "open": ".*"
      },
      "dialog": {
        "all": true
      },
      "notification": {
        "all": true
      },
      "os": {
        "all": true
      },
      "protocol": {
        "all": true
      },
      "process": {
        "all": true
      },
      "clipboard": {
        "all": true
      },
      "http": {
        "all": true,
        "request": true,
        "scope": [
          "http://**",
          "https://**"
        ]
      },
      "fs": {
        "readFile": true,
        "writeFile": true,
        "scope": [
          "**",
          "$HOME/.linksaas/**"
        ]
      }
    },
    "windows": [
      {
        "label": "main",
        "title": "",
        "width": 1000,
        "height": 640,
        "minHeight": 500,
        "minWidth": 200,
        "resizable": true,
        "fullscreen": false,
        "center": true,
        "focus": true,
        "decorations": false,
        "transparent": false,
        "fileDropEnabled": false
      }
    ],
    "systemTray": {
      "iconPath": "icons/icon.ico"
    },
    "macOSPrivateApi": true
  }
}