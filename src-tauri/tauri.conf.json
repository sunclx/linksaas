{
  "package": {
    "productName": "linksaas",
    "version": "0.1.5"
  },
  "build": {
    "distDir": "../dist",
    "devPath": "http://localhost:3000",
    "beforeDevCommand": "yarn run start",
    "beforeBuildCommand": "yarn run build",
    "withGlobalTauri": true
  },
  "tauri": {
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "pro.linksaas.desktop",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "resources": [],
      "externalBin": [
        "bin/shell"
      ],
      "copyright": "",
      "category": "DeveloperTool",
      "shortDescription": "",
      "longDescription": "",
      "deb": {
        "depends": []
      },
      "macOS": {
        "frameworks": [],
        "minimumSystemVersion": "",
        "exceptionDomain": "",
        "signingIdentity": null,
        "providerShortName": null,
        "entitlements": null
      },
      "windows": {
        "tsp": false,
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": "",
        "wix": {
          "language": "zh-CN",
          "bannerPath": "assets/wix-banner.bmp",
          "dialogImagePath": "assets/wix-dialog.bmp"
        },
        "webviewInstallMode": {
          "type": "offlineInstaller"
        }
      }
    },
    "updater": {
      "active": false
    },
    "allowlist": {
      "window": {
        "all": true
      },
      "shell": {
        "sidecar": true,
        "open": ".*",
        "scope": [
          {
            "name": "bin/shell",
            "sidecar": true,
            "args": true
          }
        ]
      },
      "dialog": {
        "all": true
      },
      "notification": {
        "all": true
      },
      "os": {
        "all": true
      },
      "protocol": {
        "all": true
      },
      "process": {
        "all": true
      },
      "clipboard": {
        "all": true
      }
    },
    "windows": [
      {
        "label": "main",
        "title": "",
        "width": 1000,
        "height": 640,
        "minHeight": 300,
        "minWidth": 600,
        "resizable": true,
        "fullscreen": false,
        "center": true,
        "decorations": false
      }
    ],
    "systemTray": {
      "iconPath": "icons/icon.ico"
    }
  }
}